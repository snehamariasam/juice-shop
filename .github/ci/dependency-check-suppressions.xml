<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.2.xsd">

    <!-- Suppress a specific CVE (false positive for CVE-2021-1234) -->
    <suppress>
        <cve>CVE-2021-1234</cve>
    </suppress>

    <!-- Suppress by CPE (false positive for example package) -->
    <suppress>
        <cpe>cpe:/a:example:example:1.0</cpe>
    </suppress>

    <!-- Suppress by CWE (cross-site scripting false positive) -->
    <suppress>
        <cwe>CWE-79</cwe>
    </suppress>

    <!-- Suppress vulnerabilities below a certain CVSS score (low severity vulnerabilities) -->
    <suppress>
        <cvssBelow>5.0</cvssBelow>
    </suppress>

</suppressions>
