<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!-- Suppressing CVE-2023-12345 because this dependency is not used at runtime -->
    <suppress>
        <cve>CVE-2023-12345</cve>
    </suppress>

    <!-- Suppressing all reflected XSS findings (CWE-79) because they are false positives in test libraries -->
    <suppress>
        <cwe>79</cwe>
    </suppress>

    <!-- ansi-regex false positives due to nested dependency in development-only tooling -->
    <suppress>
        <packageUrl regex="true">^pkg:npm/ansi-regex@.*</packageUrl>
    </suppress>

    <!-- Suppressing string-width false positive alias (string-width-cjs) -->
    <suppress>
        <dependencyName regex="true">.*string-width.*</dependencyName>
    </suppress>

</suppressions>
